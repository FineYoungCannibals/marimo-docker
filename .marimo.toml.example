[server]
browser = "default"
follow_symlink = false

[completion]
activate_on_typing = true
copilot = false

[snippets]
custom_paths = []
include_default_snippets = true

[runtime]
watcher_on_save = "lazy"
auto_reload = "off"
output_max_bytes = 8000000
reactive_tests = true
pythonpath = []
std_stream_max_bytes = 1000000
auto_instantiate = false
on_cell_change = "lazy"

[keymap]
preset = "default"

[keymap.overrides]

[package_management]
manager = "uv"

[display]
code_editor_font_size = 14
theme = "light"
cell_output = "below"
dataframes = "rich"
default_width = "medium"

[ai]
rules = ""
[ai.open_ai]
model = ""
base_url = ""
api_key = ""

[ai.anthropic]
api_key = ""

[ai.google]

[save]
autosave = "after_delay"
format_on_save = true
autosave_delay = 1000

[language_servers.pylsp]
enable_flake8 = false
enable_mypy = true
enable_pyflakes = false
enable_ruff = true
enable_pydocstyle = false
enable_pylint = false
enabled = true

[formatting]
line_length = 100

[experimental]
rtc_v2 = false
toplevel_defs = false
inline_ai_toolti